{
  "name": "zod-prisma",
  "version": "1.0.0",
  "description": "My awesome typescript library",
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@egoist/prettier-config",
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": "warn",
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "@typescript-eslint/no-non-null-assertion": "off"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "zod-prisma": "dist/cli.js"
  },
  "scripts": {
    "build-fast": "tsup-node src/cli.ts --format cjs,esm",
    "build": "pnpm run build-fast",
    "test": "vitest run",
    "test:update": "UPDATE_EXPECTED=true vitest run test/functional/driver.test.ts >/dev/null 2>&1; echo Expected outputs updated.",
    "prepublishOnly": "pnpm run build",
    "prepare": "husky install",
    "lint": "tsc --noEmit && eslint vitest.config.ts src/*.ts test/*.test.ts test/**/*.test.ts",
    "lint:fix": "tsc --noEmit && eslint --fix vitest.config.ts src/*.ts test/*.test.ts test/**/*.test.ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@egoist/prettier-config": "1.0.0",
    "@prisma/client": "^4.0.0",
    "@prisma/internals": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "decimal.js": "^10.3.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "6.1.0",
    "expect-type": "^0.13.0",
    "fast-glob": "3.2.11",
    "fs-jetpack": "4.3.1",
    "husky": "7.0.4",
    "prettier": "2.7.1",
    "prisma": "^3.15.2",
    "tsup": "6.1.2",
    "typescript": "4.7.4",
    "vite": "^2.9.13",
    "vite-plugin-eslint": "^1.6.1",
    "vitest": "^0.16.0",
    "zod": "^3.17.3"
  },
  "dependencies": {
    "@prisma/generator-helper": "^4.0.0",
    "parenthesis": "3.1.8",
    "ts-morph": "15.1.0"
  },
  "peerDependencies": {
    "decimal.js": "^10.0.0",
    "prisma": "^3.0.0 || ^4.0.0",
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "decimal.js": {
      "optional": true
    }
  }
}